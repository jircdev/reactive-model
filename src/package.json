{
	"name": "@beyond-js/reactive",
	"version": "1.2.0-beta.01",
	"title": "Reactive",
	"description": "A model for creating reactive objects with events that enable the creation of reactive models based on the Observer pattern.",
	"layout": "reactive-layout",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web-typed",
				"environment": "development",
				"backend": "backend",
				"platform": "web",
				"ports": {
					"bundles": 7530
				},
				"ts": {
					"compiler": "tsc"
				}
			},
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 8530
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/events": "^0.0.7",
		"@beyond-js/kernel": "~0.1.9",
		"uuid": "^9.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3"
	}
}
